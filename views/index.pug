extends layout

block content
  div(class='container')
    h1(class='page-header') 
      center Upload your files here! &nbsp;  
    hr
  div(class='container')
    h4 
      center Please select your file first!
    form(action='/' method='post' enctype='multipart/form-data')
      div(class='form-group')
        center
          input(type='file', name='uploadfile')
          br
          br
          input(type='submit', name='submitfule', value='Upload File')
    